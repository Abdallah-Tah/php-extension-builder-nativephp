{
    "name": "amohamed/nativephp-custom-php",
    "description": "Laravel package to install PHP extensions with NativePHP compatibility",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nativephp",
        "php-extensions",
        "static-php",
        "php-cli",
        "windows",
        "macos",
        "linux"
    ],
    "authors": [
        {
            "name": "Abdallah Mohamed",
            "email": "abdallah.mohamed@example.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Abdallah-Tah/nativephp-php-custom/issues",
        "source": "https://github.com/Abdallah-Tah/nativephp-php-custom"
    },
    "autoload": {
        "psr-4": {
            "Amohamed\\NativePhpCustomPhp\\": "src/"
        },
        "files": [
            "src/config/nativephp-custom-php.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amohamed\\NativePhpCustomPhp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amohamed\\NativePhpCustomPhp\\NativePhpCustomPhpServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0",
        "illuminate/process": "^10.0|^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Abdallah-Tah/nativephp-bin-php"
        }
    ]
}